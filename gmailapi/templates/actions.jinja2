<!DOCTYPE html>
<html>
	<head>
		<title>GMAIL API SAMPLE</title>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
		<script>
			$(document).ready(function(){
				$.get('get_message', function(data, status){
					$("#msgSnippet").html(data['snippet']);
					$("#message").html(data['message']);
				})
			})

		</script>
	</head>
	<body>
		<font face="calibri">
		<h1>
			<a href="messages">VIEW MESSAGE</a><br>
			<a href="send_message_view">SEND MESSAGE</a>
		</h1>
		</font>
	</body>
</html>